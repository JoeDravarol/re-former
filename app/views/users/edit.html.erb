<h1>Edit User</h1>

<%= form_for @user do |f| %>
  <%= f.label :username, "Username:" %>
  <%= f.text_field :username, placeholder: "Your username here" %>

  <%= f.label :email, "Email:" %>
  <%= f.email_field :email %>

  <%= f.label :password, "Password:" %>
  <%= f.password_field :password %>
  <%= f.submit "Submit" %>
<% end %>